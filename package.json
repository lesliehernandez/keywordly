{
  "name": "keywordly",
  "version": "1.0.0",
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@coreui/coreui": "^2.0.4",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.2.0",
    "@fortawesome/fontawesome-free": "^5.1.1",
    "@material-ui/core": "^1.4.0",
    "aos": "^2.3.3",
    "async": "^2.6.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "json2csv": "^4.1.6",
    "jsonwebtoken": "^8.3.0",
    "material-ui": "^0.20.1",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.3",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.18.3",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "reactstrap": "^6.3.0",
    "uuid": "^3.2.1",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
